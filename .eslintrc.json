{
  "env": {
    "browser": false
  },
  "extends": ["airbnb", "prettier/react", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "rules": {
    "react/prop-types": 0,

    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "jsxBracketSameLine": false
      }
    ]
  },
  "plugins": ["prettier"],
  "globals": {"fetch": false}
}
