{
  "env": {
    "browser": false
  },
  "extends": ["airbnb", "prettier/react", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "rules": {
    "react/prop-types": 0,
    "no-console": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  },
  "plugins": ["prettier"],
  "globals": {"fetch": false}
}
